{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "pretest": "cross-env NODE-ENV=test npm run migrate:reset",
    "test": "cross-env NODE-ENV=test jest --coverage --testTimeout=10000 --detectOpenHandles",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate && npx sequelize-cli db:seed:all"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__test__/"
    ]
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "bcrypt": "^4.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "^4.0.0",
    "crypto-random-string": "^3.2.0",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "googleapis": "39",
    "helmet": "^3.22.0",
    "http-errors": "~1.6.3",
    "http-status": "^1.4.2",
    "js-base64": "^2.5.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "morgan": "~1.9.1",
    "pg": "^7.18.2",
    "redis": "^3.0.2",
    "rotating-file-stream": "^2.0.2",
    "sequelize": "^5.21.5",
    "uuidv4": "^6.0.6",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "jest": "^25.2.7",
    "supertest": "^4.0.2"
  }
}
